<% if current_user == @user %>
 	<% if  params[:tab] == "mycars" %>
  	<%= render 'my_cars' %>
	<% elsif  params[:tab] == "myrents" %>
	  <%= render 'my_rents' %>
	<% elsif params[:tab] == "myprofile" %>
	  <%= render 'my_profile' %>
	 <% elsif params[:tab] == "mybooking" %>
	 	<%= render 'my_cars_booking' %>
  <% end %>
<% else %>
  <%= render 'public_profile' %>
<% end %>